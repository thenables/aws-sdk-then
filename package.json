{
  "name": "aws-sdk-then",
  "description": "Promisify the AWS SDK",
  "version": "1.1.0",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jonathanong/aws-sdk-then",
  "dependencies": {
    "any-promise": "^1.1.0",
    "memorizer": "^1.0.1"
  },
  "devDependencies": {
    "aws-sdk": "^2.2.30",
    "babel-eslint": "^5.0.0",
    "codecov": "^1.0.0",
    "eslint": "^2.3.0",
    "eslint-config-jongleberry": "^2.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.0.0",
    "ghooks": "^1.0.0",
    "mocha": "^2.0.0",
    "nyc": "^6.0.0"
  },
  "scripts": {
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint lib test",
    "test": "mocha --bail",
    "test-cov": "nyc --cache node_modules/mocha/bin/_mocha && nyc report --reporter=lcov"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "keywords": [
    "aws",
    "sdk",
    "then",
    "promise"
  ],
  "files": [
    "lib"
  ],
  "main": "lib"
}
